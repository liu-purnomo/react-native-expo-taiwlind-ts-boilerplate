{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "paths": {
      "app/*": ["./app/*"],
      "src/*": ["./src/*"],
      "api/*": ["./src/api/*"],
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "constants/*": ["./src/constants/*"],
      "screens/*": ["./src/screens/*"]
    }
  }
}
